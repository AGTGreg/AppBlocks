{
  "name": "appblocks",
  "version": "2.0.3",
  "description": "A lightweight javascript library for building micro apps for the front-end.",
  "main": "dist/appblocks.cjs.js",
  "module": "dist/appblocks.esm.js",
  "browser": "dist/appblocks.umd.js",
  "minified": "dist/appblocks.min.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:core": "jest tests/core",
    "test:directives": "jest tests/directives",
    "test:filters": "jest tests/filters",
    "test:utils": "jest tests/utils",
    "test:processing": "jest tests/processing",
    "test:requests": "jest tests/requests",
    "build": "rollup --config",
    "pretest": "npx babel src --out-dir build-cjs --extensions \".js\" --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AGTGreg/AppBlocks.git"
  },
  "keywords": [
    "appblocks",
    "microapps",
    "frontend",
    "interface",
    "HTML"
  ],
  "author": "Grigoris Chatzinikolaou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AGTGreg/AppBlocks/issues"
  },
  "homepage": "https://agtgreg.github.io/AppBlocks",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-terser": "^0.4.4",
  "babel-jest": "^30.2.0",
  "@babel/cli": "^7.9.0",
    "core-js": "^3.36.0",
    "idiomorph": "^0.3.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.1.0",
    "rollup": "^2.3.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-gzip": "^2.4.0"
  }
}
